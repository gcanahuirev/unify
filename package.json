{
  "name": "unify-api",
  "version": "0.1.0",
  "license": "MIT",
  "description": "API descentralizada",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "start": "nest start",
    "build": "nest build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "predocs": "rimraf documentation",
    "docs": "compodoc -p tsconfig.json --theme laravel -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": "git@github.com:gcanahuirev/unify.git",
  "bugs": {
    "url": "https://github.com/gcanahuirev/unify/issues"
  },
  "homepage": "https://github.com/gcanahuirev/unify#readme",
