    "fastify-helmet": "^5.3.2",
    "fastify-swagger": "^4.12.6",
    "joi": "^17.4.2",
    "nanoid": "^3.1.30",
    "nest-access-control": "^2.0.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pino-pretty": "^7.2.0",
