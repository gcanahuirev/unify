    "@nestjs/common": "^8.2.2",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-fastify": "^8.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
